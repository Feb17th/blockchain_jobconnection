        "sort-keys-recursive": "^2.1.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.3.0",
        "@tsconfig/node18": "^18.2.4",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/node": "^18.19.33",
        "eslint": "^8.57.0",
        "typescript": "~5.4.5",
        "typescript-eslint": "^7.11.0",
        "nodemon": "^3.1.4"
